<category>
      <pattern>HOME</pattern>
      <template>
         <think><set name = "state">IntentHome</set> </think>
      <condition name = "state" value = "IntentHome">
         <random>
            <li> OK, I will go home right now,and the current state is IntentHome </li>
            <li> OK, Sure, let's go home,and the current state is IntentHome </li>
            <li> No problem, I am on my way home,and the current state is IntentHome </li>
         </random>
      </condition>
        


      </template>
   </category>  
   
