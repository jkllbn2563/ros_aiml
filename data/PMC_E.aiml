<category>
      <pattern>COMPLETE</pattern>
      <template>
      	<think><set name = "state">IntentDelivery</set> </think>
      <condition name = "state" value = "IntentDelivery">
         <random>
            <li> Cellphone is complete, and I will stack it,and the current state is IntentDelivery </li>
            <li> I got it! ready to send the product,and the current state is IntentDelivery </li>
         </random>
      </condition>
        
      	


      </template>
   </category>  
   
