<category><pattern>_ APPLE TEST</pattern>
<template>OK! just for test.</template>
</category>



 <category>
      <pattern>CAMERA</pattern>
      <template>
      	<think><set name = "item">camera</set></think>
      	<think><set name = "state">IntentFind</set></think>
      
        <condition name = "state" value = "IntentFind">
            
            <random>
            <li> OK, I will pick the material of <get name = "item"/> for you,and the current state is IntentFind </li>
            <li> I will collect the parts of camera,and the current state is IntentFind </li>
            <li> Sure, I can do it,and the current state is IntentFind </li>
            </random>
        </condition>

         
      	
      </template>
   </category>  
   
   <category>
      <pattern>_ DO * CAMERA *</pattern>
      <template>
         <srai>CAMERA</srai>
      </template>
   </category>

   <category>
      <pattern>_ PACKAGE * CAMERA</pattern>
      <template>
         <srai>CAMERA</srai>
      </template>
   </category>
   
   <category>
      <pattern>_ GIVE * CAMERA</pattern>
      <template>
         <srai>CAMERA</srai>
      </template>
   </category>
   
   


<category><pattern>_ ARE YOU DOING</pattern>
<template>I am delivering the item.</template>
</category>

<category><pattern>_ TASK ARE YOU *</pattern>
<template>I am delivering the material of <get name = "item"/>.</template>
</category>

<category><pattern>_ MANY CAMERA *</pattern>
<template>There are two at the shipping area.</template>
</category>

<category>
      <pattern>_ HAVE BEEN FINISHED</pattern>
      <template>
      <think><set name = "state">IntentDelivery</set> </think>
      <condition name = "state" value = "IntentDelivery">
         <random>
            <li> OK, I will deliver it to the shipping area,and the current state is IntentDelivery </li>
            <li> OK, I will take it,and the current state is IntentDelivery </li>
            <li> No preblem, I will do it,and the current state is IntentDelivery </li>
         </random>
      </condition>
      </template>

</category>




