<category><pattern>HOW CAN I TEST *</pattern>
<template>OK! just for test.</template>
</category>



 <category>
      <pattern>BOX</pattern>
      <template>
      	<think><set name = "item">BOX</set></think>
      	<think><set name = "state">IntentFind</set></think>
      
        <condition name = "state" value = "IntentFind">
            
            <random>
            <li> BOX is consist of the A,B and C,and the current state is IntentFind. </li>
            <li> I got it! ready to pick the BOX,and the current state is IntentFind </li>
            </random>
        </condition>

         
      	
      </template>
   </category>  
   
   <category>
      <pattern>_ BOX</pattern>
      <template>
         <srai>BOX</srai>
      </template>
   </category>
   
   <category>
      <pattern>_ BOX</pattern>
      <template>
         <srai>BOX</srai>
      </template>
   </category>
   
   <category>
      <pattern>BOX *</pattern>
      <template>
         <srai>BOX</srai>
      </template>
   </category>
   
   <category>
      <pattern>_ BOX *</pattern>
      <template>
         <srai>BOX</srai>
      </template>
   </category>











