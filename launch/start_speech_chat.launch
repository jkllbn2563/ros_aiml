<launch>

	<!--assistant related-->
  <param name="aiml_path" value="$(find ros_aiml)/data" />
  <node name="aiml_server" pkg="ros_aiml" type="aiml_server.py" output="screen"/>

  <!--vocal output related, offline-->
	<!--include file="$(find sound_play)/soundplay_node.launch"/>
	<node name="aiml_tts_soundplay" pkg="ros_aiml" type="aiml_tts_soundplay.py"/-->

	<!--vocal output related, online-->
	<node name="aiml_tts_google" pkg="ros_aiml" type="aiml_tts_google.py"/>

  <!--vocal input related, need isolately run-->
  <!--node name="aiml_stt_google" pkg="ros_aiml" type="aiml_stt_google.py" output="screen"/-->

</launch>
